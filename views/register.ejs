<div class="contains">
    <img src="/image/tamanna-rumee-TOigBgviy_g-unsplash.jpg" alt="">
        <div class="formj">
            <div class="form">
                <form action="/auth/register" method="POST">
                    <h2>Welcome to Maed</h2>
                    <p>Let's get you signed up</p>
                    <input type="text" placeholder="Name" name="fullname" value="<%= typeof fullname!='undefined' ? fullname : '' %>" required><br>
                    <input type="email" placeholder="Email" name="username" value="<%= typeof email!='undefined' ? email: ''%>" required><br>
                    <select name = "role">
                        <option value="user" id="options">User</option>
                        <option value="medical"id="options">Medical Professional</option>
                    </select>
                    <input type="password" placeholder="Password" name="password" value="<%= typeof password!='undefined' ? password: ''%>" required><br>
                    <button type="submit" id="button">Register</button>
                </form>
                <p id="question">do you  already have an account? <a href="/auth/login">login</a></p>
            </div>
        </div>